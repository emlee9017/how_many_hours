<div class="page-header">
  <h3>Completion #<%= @completion.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @completion.user_id %></dd>

      <dt>Course</dt>
      <dd><%= @completion.course_id %></dd>

      <dt>Activity</dt>
      <dd><%= @completion.activity_id %></dd>

      <dt>Duration</dt>
      <dd><%= @completion.duration %></dd>

      <dt>Completion</dt>
      <dd><%= @completion.completion %></dd>

      <dt>Notes</dt>
      <dd><%= @completion.notes %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/completions" class="btn btn-primary">
        Back
      </a>
      <a href="/completions/<%= @completion.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_completion/<%= @completion.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
