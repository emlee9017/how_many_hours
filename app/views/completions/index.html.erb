<div class="page-header">
  <h1>
    Completions
    <a href="/completions/new" class="btn btn-lg btn-success">New Completion</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
        <th>Activity</th>
        <th>Duration</th>
        <th>Completion</th>
        <th>Notes</th>
        <th>Actions</th>
      </tr>

      <% @completions.each do |completion| %>
      <tr>
        <td><%= completion.user_id %></td>
        <td><%= completion.activity_id %></td>
        <td><%= completion.duration %></td>
        <td><%= completion.completion %></td>
        <td><%= completion.notes %></td>
        <td>
          <a href="/completions/<%= completion.id %>" class="btn btn-primary">Show</a>
          <a href="/completions/<%= completion.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_completion/<%= completion.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
